<!--components/movie-list/index.wxml-->
<view class="container d-class">
  <view class="title-container">
    <text>{{title}}</text>
    <text hover-class="hover">更多 ></text>
  </view>
  <view class="movie-container">
    <block wx:for="{{movies}}" wx:key="index">
      <movie movie="{{item}}"/>
    </block>
  </view>
</view>

